{
  "manifest_version": 2,
  "name": "SafariBlocker",
  "version": "1.0",
  "description": "Blocks unwanted new tabs and allows whitelisting/blacklisting.",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
